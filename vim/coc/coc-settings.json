{
  "suggest.noselect": false,
  "javascript.suggestionActions.enabled": false,
  "snippets.ultisnips.enable": true,
  "snippets.ultisnips.directories": ["~/dotfiles/vim/ultisnips"],
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    }
  }
}
