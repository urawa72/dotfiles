{
  "codeLens.enable": false,
  "coc.preferences.colorSupport": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "prisma",
    "tsx"
  ],
  "stylelintplus.cssInJs": true,
  "eslint.autoFixOnSave": true,
  "suggest.noselect": false,
  "javascript.suggestionActions.enabled": false,
  "snippets.ultisnips.enable": true,
  "snippets.ultisnips.directories": ["~/dotfiles/vim/ultisnips"],
  "python.linting.lintOnSave": true,
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.mypyEnabled": true,
  "pyright.organizeimports.provider": "isort",
  "languageserver": {
    "efm": {
      "command": "efm-langserver",
      "args": [],
      "filetypes": ["markdown"]
    }
  },
  "yaml.schemas": {
    "https://raw.githubusercontent.com/awslabs/goformation/v4.18.2/schema/cloudformation.schema.json": [
      "*.template.yaml"
    ]
  },
  "yaml.customTags": [
    "!Ref",
    "!GetAtt",
    "!fn",
    "!And",
    "!If",
    "!Not",
    "!Equals",
    "!Or",
    "!FindInMap sequence",
    "!Base64",
    "!Cidr",
    "!Sub",
    "!GetAZs",
    "!ImportValue",
    "!Select",
    "!Split",
    "!Join sequence"
  ],
  "yaml.validate": true,
  "yaml.completion": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.file.showHiddenFiles": true,
  "explorer.previewAction.onHover": false,
  "explorer.diagnostic.displayMax": 0,
  "explorer.diagnostic.enableSubscriptNumber": false,
  "java.autobuild.enabled": false,
  "rust-analyzer.updates.prompt": false,
  "rust-analyzer.cargo.runBuildScripts": true,
  "rust-analyzer.procMacro.enable": true,
  // "rust-analyzer.server.path": "~/.config/coc/extensions/coc-rust-analyzer-data/manualy_download/rust-analyzer-20220110",
  "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/13.0.0/clangd_13.0.0/bin/clangd",
  "sql.database": "postgresql"
}
